[alias]
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --
    co = checkout
    ci = commit
    st = status
    br = branch
    hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
    type = cat-file -t
    dump = cat-file -p
    brv = branch -vv
    bra = branch -a
    bl = blame
    cm = checkout master
    brd = branch -D
    cim = commit -m
[user]
	name = Mateusz Gorski
	email = mateu.gorski@gmail.com
[core]
	editor = vim
[merge]
	tool = vimdiff
[push]
	default = simple
